rules_version = '2';
service firebase.storage {
  match /{imageId} {
    allow read: if resource.size < 100 * 1024;
    allow write: if imageId.matches(".*\\.txt");
  }

  match /traces/{traceId} {
    allow read
    allow write
  }
  match /b/{bucket}/o {
    match /{allPaths=**} {
      allow read, write: if false;
    }
  }
}
